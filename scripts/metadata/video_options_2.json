{
  "format": "bestvideo+bestaudio",
  "progress": true,
  "postprocessors": [
    {
      "already_have_subtitle": false,
      "key": "FFmpegEmbedSubtitle"
    }
  ],
  "postprocessor_args": ["-c:v", "libx264", "-c:a", "aac"],
  "writesubtitles": true,
  "writeautomaticsub": true,
  "subtitleslangs": ["en"],
  "outtmpl": "%(title)s.%(ext)s"
}
